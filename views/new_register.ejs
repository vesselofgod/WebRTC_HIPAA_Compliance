        <section class="page-section" id="contact">
            <div class="container">
                <div class="text-center">
                    <h2 class="section-heading text-uppercase">회원가입</h2>
                </div>
                <div class="login-page-text">
                    <h3 class="section-subheading text-muted">필요한 정보들을 입력해주세요.</h3>
                </div>
                <!-- * * * * * * * * * * * * * * *-->
                <!-- * * SB Forms Contact Form * *-->
                <!-- * * * * * * * * * * * * * * *-->
                <!-- This form is pre-integrated with SB Forms.-->
                <!-- To make this form functional, sign up at-->
                <!-- https://startbootstrap.com/solution/contact-forms-->
                <!-- data-sb-form-api-token="API_TOKEN"-->
                <form method="post" id="contactForm" >
                    <div class="row align-items-stretch mb-5">
                        <div class="col-md-3"></div>
                        <div class="col-md-3">
                            <div class="form-group form-group-textarea mb-md-0">
                                <!-- Message input-->
                                <h2 class="register-text-form" style="color:white" >Name</h2>
                                <h2 class="register-text-form" style="color:white">Age</h2>
                                <h2 class="register-text-form" style="color:white">ID</h2>
                                <h2 class="register-text-form" style="color:white">Password</h2> 
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <!-- Name input-->
                                <input class="form-control" name="name" type="text" placeholder="Your Name *" required/>
                                <!-- <div class="invalid-feedback" data-sb-feedback="name:required">A name is required.</div> -->
                            </div>
                            <div class="form-group">
                                <!-- Name input-->
                                <input class="form-control" name="age" type="text" placeholder="Your Age *" required/>
                                <!-- <div class="invalid-feedback" data-sb-feedback="name:required">An age is required.</div> -->
                            </div>
                            <div class="form-group">
                                <!-- Email address input-->
                                <input class="form-control" name="id" type=text" placeholder="Your Id *" required/>
                                <!-- <div class="invalid-feedback" data-sb-feedback="email:required">An Id is required.</div> -->
                            </div>
                            <div class="form-group mb-md-0">
                                <!-- Phone number input-->
                                <input class="form-control" name="pw" type="password" placeholder="Your Password *"  required/>
                                <!--<div class="invalid-feedback" data-sb-feedback="phone:required">A password is required.</div>-->
                            </div>
                            <br />
                            
                        </div>
                      
                        <!-- Phone number input-->
<!--                         <div class="col-md-6">
                            <div class="form-group form-group-textarea mb-md-0">
                                <!-- Message input-->
                                <!--  <textarea class="form-control" id="message" placeholder="Your Message *" data-sb-validations="required"></textarea>
                                <div class="invalid-feedback" data-sb-feedback="message:required">A message is required.</div>
                            </div>
                        </div> -->
                    </div>
                    <!-- Submit success message-->
                    <!---->
                    <!-- This is what your users will see when the form-->
                    <!-- has successfully submitted-->
                    <div class="d-none" id="submitSuccessMessage">
                        <div class="text-center text-white mb-3">
                            <div class="fw-bolder">회원가입이 완료되었습니다</div>
                        </div>
                    </div>
                    <!-- Submit error message-->
                    <!---->
                    <!-- This is what your users will see when there is-->
                    <!-- an error submitting the form-->
                    <div class="d-none" id="submitErrorMessage"><div class="text-center text-danger mb-3">중복된 ID입니다.</div></div>
                    <!-- Submit Button-->
                    <div class="text-center"><input class="btn btn-primary btn-xl text-uppercase" value="회원가입"  id="submitButton" type="submit"></div>
                    <!--class="btn btn-primary btn-xl text-uppercase disabled" -->
